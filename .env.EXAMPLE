# Local Development
NODE_ENV=development
DATABASE_URL=your_local_database_url  # e.g., postgres://username:password@localhost:5432/your_db_name
SESSION_SECRET=your_local_session_secret  # e.g., a random string for session management

# Production Deployment
NODE_ENV=production
DATABASE_URL=your_render_database_url  # e.g., postgres://username:password@render.com/your_db_name
SESSION_SECRET=your_render_session_secret  # e.g., a random string for session management